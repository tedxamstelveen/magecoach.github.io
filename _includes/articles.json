{
  "@context": "http://schema.org",
  "@type": "NewsArticle",
  "mainEntityOfPage": {
    "@type": "WebPage",
    "@id": "{{page.url}}"
  },
  "headline": "{{page.title}}",
  "image": {
    "@type": "ImageObject",
    "url": "{{site.static-url}}{{page.image}}",
    "height": 800,
    "width": 800
  },
  "datePublished": "{{page.datePublished}}",
  "dateModified": "{{page.dateModified}}",
  "author": {
    "@type": "Person",
    "name": "{{page.authorname}}"
  },
   "publisher": {
    "@type": "Organization",
    "name": "{{site.title}}",
    "logo": {
      "@type": "ImageObject",
      "url": "{{site.url}}{{page.image}}",
      "width": "{{page.width}}",
      "height": "{{page.height}}"
    }
  },
  "description": "{{post.description}}"
}
